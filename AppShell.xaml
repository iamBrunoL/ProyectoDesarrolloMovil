<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProyectoDesarrolloMovil.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProyectoDesarrolloMovil"
    Shell.FlyoutBehavior="Disabled"
    Title="ProyectoDesarrolloMovil">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="LoginPage" />
    <ShellContent Title="Panel de Control" ContentTemplate="{DataTemplate local:PanelControlPage}" />
    <ShellContent Title="Mis Datos" ContentTemplate="{DataTemplate local:MisDatosPage}" />


</Shell>
